# UDP server and client commands
us:
	python3 UDP/main_udp_server.py
uc:
	python3 UDP/main_udp_client.py


# TCP server and client commands
ts:
	python3 TCP/main_tcp_server.py
tc:
	python3 TCP/main_tcp_client.py

